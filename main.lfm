      OnExecute = actCreateExecute
    end
    object actSave: TAction
      Hint = 'Сохранить задачу'
      ImageIndex = 1
      OnExecute = actSaveExecute
    end
    object actExit: TAction
      Hint = 'Выйти из программы'
      ImageIndex = 4
      OnExecute = actExitExecute
    end
  end
end
